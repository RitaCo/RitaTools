{
    "name": "ritaco/tools",
    "type": "cakephp-plugin",
    "description": "some library for CakePHP by used in RitaCo Projects.",
    "keywords": ["cakephp", "plugin", "tools"],
    "homepage": "https://github.com/RitaCo/cakephp-users-plugin",
    "license": "MIT",
    "version": "3.0.1-dev",
    "authors": [
        {
            "name": "Saleh Souzanchi",
            "role": "Author",
            "homepage": "http://www.ritaco.net"
        }
    ],
    "require":{
        "php": ">=5.4"
    },
    "require-dev": {
        "cakephp/cakephp": "3.0.*-dev",
        "phpunit/phpunit": "4.1.*"
    },    
    "autoload": {
        "psr-4": {
            "Rita\\Tools\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rita\\Tools\\Test\\": "tests"
        }
    },
    "support":{
        "source": "https://github.com/RitaCo/cakephp-tools-plugin",
        "issues": "https://github.com/RitaCo/cakephp-tools-plugin/issues"
    },
    "extra": {
        "installer-name": "Rita/Tools"
    }
}
